config 'global'
	option	anon_swap       '0'
	option	anon_mount      '0'
	option	auto_swap       '1'
	option	auto_mount      '1'
	option	delay_root      '5'
	option  check_fs        '0'

config 'mount'
	option  target  	'/mnt/UDISK'
	option	device		'/dev/by-name/UDISK'
	option	fstype		'ext4'
	option	options		'rw,sync'
	option	enable		'1'
	option	enable_fsck	'1'

config 'mount'
	option  target  	'/overlay'
	option	device		'/dev/by-name/rootfs_data'
	option	fstype		'ext4'
	option	options		'rw,sync'
	option	enable		'1'
	option	enable_fsck	'1'
